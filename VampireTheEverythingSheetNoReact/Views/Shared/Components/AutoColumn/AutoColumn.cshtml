@using VampireTheEverythingSheetNoReact.Shared_Files
@{
    Trait[] traits = ViewData["Traits"] as Trait[] ?? [];
    int column = ViewData["Column"] as int? ?? 1;
    int row = ViewData["StartRow"] as int? ?? 0;

    foreach(Trait trait in traits)
    {
        @(
        await Component.InvokeAsync("GridElement", new
        {
            contents = Component.InvokeAsync("TraitRenderer", trait),
            column = column,
            row = row,
            width = 1,
            height = 1
        })
        )
        row++;
    }
}